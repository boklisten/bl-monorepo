{
  "name": "frontend",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "build:tuyau": "pnpm exec tsc --project ../backend/tsconfig.tuyau.json --noResolve --noCheck",
    "build:shared": "pnpm exec tsc --build ../backend/shared",
    "build:external": "pnpm exec concurrently \"pnpm run build:shared\" \"pnpm run build:tuyau\"",
    "dev": "pnpm build:external && next dev",
    "typecheck": "pnpm build:external && pnpm exec tsc --noEmit",
    "build": "pnpm build:external && next build",
    "serve": "next start"
  },
  "dependencies": {
    "@date-io/moment": "^3.2.0",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.1.1",
    "@mui/icons-material": "^6.4.1",
    "@mui/lab": "6.0.0-beta.24",
    "@mui/material": "^6.4.1",
    "@mui/material-nextjs": "^6.3.1",
    "@mui/x-date-pickers": "^7.24.0",
    "@sentry/nextjs": "^8.50.0",
    "@toolpad/core": "^0.12.0",
    "@tuyau/client": "^0.2.4",
    "@yudiel/react-qr-scanner": "^2.1.0",
    "draft-js": "^0.11.7",
    "moment": "^2.30.1",
    "next": "15.1.5",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-draft-wysiwyg": "^1.15.0",
    "react-hook-form": "^7.54.2",
    "react-jwt": "^1.2.2",
    "react-quill": "^2.0.0",
    "sanitize-html": "^2.14.0",
    "sharp": "^0.33.5",
    "string-similarity": "^4.0.4",
    "swr": "^2.3.0",
    "validator": "^13.12.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@testing-library/react": "^16.2.0",
    "@tsconfig/cypress": "^1.0.2",
    "@tsconfig/next": "^2.0.3",
    "@types/draft-js": "^0.11.18",
    "@types/react": "19.0.7",
    "@types/react-draft-wysiwyg": "^1.13.8",
    "@types/sanitize-html": "^2.13.0",
    "@types/string-similarity": "^4.0.2",
    "@types/validator": "^13.12.2",
    "cypress": "^14.0.0"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ]
}
